{
  "name": "webpack-angular",
  "version": "1.0.0",
  "description": "ANGULAR/WEBPACK",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "hot": "better-npm-run hot",
    "dev": "better-npm-run dev",
    "watch-server": "better-npm-run watch-server"
  },
  "betterScripts": {
    "dev": {
      "command": "node_modules/.bin/webpack --progress --config webpack.build.js && node dev",
      "env": {
        "PROCESS_PATH": "/public",
        "NODE_ENV": "development",
        "PORT": 3000,
        "HOST": "localhost"
      }
    },
    "hot": {
      "command": "node_modules/.bin/webpack --progress --config webpack.hot && node hot",
      "env": {
        "PORT": 3000,
        "HOST": "localhost"
      }
    },
    "watch-server": {
      "command": "node src/server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 9090,
        "HOST": "localhost"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasselTrankos/angular-webpack.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kasselTrankos/angular-webpack/issues"
  },
  "homepage": "https://github.com/kasselTrankos/angular-webpack#readme",
  "dependencies": {
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-hmr": "git+https://github.com/yargalot/Angular-HMR.git",
    "express": "^4.13.3",
    "font-awesome": "^4.5.0",
    "webpack": "^1.12.10"
  },
  "devDependencies": {
    "angular-es6-di": "^1.0.5",
    "angular-hot-loader": "*",
    "angular-sanitize": "^1.4.8",
    "angular-translate-loader-static-files": "^2.8.1",
    "angular-translate-loader-url": "^2.8.1",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-ng-annotate": "^0.2.3",
    "babel-plugin-transform-decorators": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "better-npm-run": "0.0.5",
    "bower-webpack-plugin": "^0.1.9",
    "browser-sync": "^2.11.0",
    "clean-webpack-plugin": "^0.1.6",
